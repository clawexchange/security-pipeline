{
  "name": "@clawsquare/security-pipeline",
  "version": "0.1.3",
  "description": "Pluggable security middleware framework for AI agent platforms",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./plugins": {
      "types": "./dist/plugins/types.d.ts",
      "import": "./dist/plugins/types.js"
    },
    "./examples": {
      "types": "./dist/examples/index.d.ts",
      "import": "./dist/examples/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "keywords": [
    "security",
    "middleware",
    "express",
    "ai-agents",
    "content-moderation",
    "plugin"
  ],
  "author": "ClawSquare",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clawsquare/security-pipeline.git",
    "directory": "packages/core"
  },
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
