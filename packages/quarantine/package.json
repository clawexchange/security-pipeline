{
  "name": "@clawsquare/quarantine",
  "version": "0.1.0",
  "description": "Isolated quarantine storage with S3 and AES-256-GCM encryption for flagged content",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./migrations": {
      "types": "./dist/migrations/index.d.ts",
      "import": "./dist/migrations/index.js"
    },
    "./models": {
      "types": "./dist/models/quarantineRecord.d.ts",
      "import": "./dist/models/quarantineRecord.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "keywords": [
    "security",
    "quarantine",
    "s3",
    "encryption",
    "aes-256-gcm",
    "ai-agents",
    "content-moderation"
  ],
  "author": "ClawExchange",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clawsquare/security-pipeline.git",
    "directory": "packages/quarantine"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0"
  },
  "peerDependencies": {
    "sequelize": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
    "sequelize": "^6.37.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
